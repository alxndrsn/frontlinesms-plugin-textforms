<?xml version="1.0" encoding="ISO-8859-1"?>
<panel i18n="true" name="panelBrowseData" weightx="1" weighty="1" scrollable="false" columns="3" gap="10" left="6" right="6" bottom="6">
	<panel gap="5" weightx="1">
		<label name="labelFieldName" text="i18n.plugins.resourcemapper.fieldname" />
		<textfield name="textSearchName" weightx="1" perform="searchFieldName()"/>
	</panel>
	
	<panel gap="5" left="15">
		<textfield name="textDate" editable="false" enabled="false" columns="15"/>
		<button name="buttonDate" icon="/icons/date.png" action="showDateSelecter(textDate)"/>
    </panel>
    
    <panel gap="5" weightx="1" left="15">
    	<label name="labelSubmitter" text="i18n.plugins.resourcemapper.submitter" />
		<combobox name="comboSubmitter" editable="false" weightx="1" />
	</panel>
	
	<table name="tableFields" selection="single" colspan="3" weightx="1" weighty="1">
		<header>
			<column icon="/icons/file.png" text="i18n.plugins.resourcemapper.table.field" width="120" />
			<column icon="/icons/sms_receive.png" text="i18n.plugins.resourcemapper.table.response" width="210"/>
			<column icon="/icons/date.png" text="i18n.plugins.resourcemapper.table.date" width="150"/>
			<column icon="/icons/user_sender.png" text="i18n.plugins.resourcemapper.table.submitter" width="200"/>
		</header>
	</table>	
</panel>